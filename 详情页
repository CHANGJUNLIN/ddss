<script>
// Measure a view of product details. This example assumes the detail view occurs on pageload,
// and also tracks a standard pageview of the details page.
dataLayer.push({
  'ecommerce': {
    'detail': {
      'actionField': {'list': 'Hotel'},    // 'detail' actions have an optional list property. 根据机酒车的不同设置不同的list name: Hotel, Flight, Car
      'products': [{
       'name': '',      
       'id': '',
       'price': '',
       'brand': 'operatorname_en',
       'category': 'countryname_en',
       'variant': 'cityname_en'
       }]
     }
   }
});
</script>
